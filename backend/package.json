{
  "name": "amused-backend",
  "version": "2.0.0",
  "description": "Simple Node.js backend for AMused products management with real-time events",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec \"npm run build && node dist/index.js\"",
    "build": "tsc && tsc src/workers/analyticsWorker.ts --outDir dist/workers",
    "start": "node dist/index.js",
    "clean": "rimraf dist",
    "db:up": "docker-compose up -d postgres",
    "db:down": "docker-compose down"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "postgresql",
    "sse",
    "real-time",
    "api",
    "products"
  ],
  "author": "AMused Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@jest/globals": "^30.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "cross-env": "^7.0.3",
    "jest": "^30.1.3",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "typescript": "^5.7.2"
  }
}
